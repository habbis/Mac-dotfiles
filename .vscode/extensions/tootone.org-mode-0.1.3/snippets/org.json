{
    "title": {
        "prefix": "title",
        "body": "#+TITLE: ${1:title text}",
        "description": "Document title"
    },
    "options": {
        "prefix": "options",
        "body": "#+OPTIONS: ${1:option text}",
        "description": "Document options"
    },
    "toc": {
        "prefix": "toc",
        "body": "#+TOC: ${1:toc text}",
